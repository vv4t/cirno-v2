i32 n = 5;
i32[n] data;

data[0] = 10;
data[1] = 1;
data[2] = 234;
data[3] = 5;
data[4] = 4;

i32 tmp;

i32 i = 0;
i32 j = 0;
while (i < n) {
  j = 0;
  
  while (j < n - i - 1) {
    if (data[j] > data[j + 1]) {
      tmp = data[j];
      data[j] = data[j + 1];
      data[j + 1] = tmp;
    }
    j = j + 1;
  }
  
  i = i + 1;
}

i = 0;
while (i < n) {
  print(data[i]);
  i = i + 1;
}
