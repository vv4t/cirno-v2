class_def vec3 {
  f32 x;
  f32 y;
  f32 z;
};

fn vec3_init(f32 x, f32 y, f32 z) : class vec3
{
  class vec3 e = new vec3;
  class vec3 v = new vec3;
  v.x = x;
  v.y = y;
  v.z = z;
  return v;
}

fn main()
{
  i32 i = 0;
  while (i < 5) {
    class vec3 v = vec3_init(0.0, 0.0, 0.0);
    i = i + 1;
  }
}

main();
